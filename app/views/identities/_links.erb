<div id="omniauth_links">
	<ul>
	  <%- User.omniauth_providers.each do |provider| %>
	    <li style="float:left;">
			<%= link_to omniauth_authorize_path(resource_name, provider), :class => "provider" do %>
				<%= provider_image provider, 64 %>
				<%= provider.to_s.camelcase %>
			<% end %>
		</li>
	  <% end -%>
	</ul>
</div>